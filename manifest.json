{
  "manifest_version": 2,

  "name": "Aliaser",
  "description": "Allows users to set aliases",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.12.0.min.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": ["storage", "http://*/*", "https://*/*"]
}